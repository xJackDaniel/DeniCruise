<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GlobalCruisePage">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="61dp"
        android:layout_height="53dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="14dp"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/global_icon" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="54dp"
        android:fontFamily="@font/norwester"
        android:text="Global Cruises: "
        android:textColor="#000000"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="339dp"
        android:layout_height="145dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@drawable/global_cruise_1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="270dp"
        android:layout_height="95dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="70dp"
        android:fontFamily="@font/norwester"
        android:text="Mystical Cruise - Israel, Italy, Spain And Maroco\n\n            Starting at 1500$ per person"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <Button
        android:id="@+id/button2"
        android:layout_width="163dp"
        android:layout_height="43dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#878787"
        android:text="Read More"
        android:onClick="MysticalCruiseDialog"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="163dp"
        android:layout_height="43dp"
        android:layout_marginStart="123dp"
        android:layout_marginEnd="125dp"
        android:backgroundTint="#878787"
        android:text="Read More"
        android:onClick="MoonCruiseDialog"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.085" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="265dp"
        android:layout_height="196dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="74dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/global_cruise_2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="271dp"
        android:layout_height="46dp"
        android:layout_marginStart="86dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="54dp"
        android:fontFamily="@font/norwester"
        android:text="Moon Cruise - Israel, Portugal, Brazil And Argentina\n\n            Starting at 2500$ per person"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

</androidx.constraintlayout.widget.ConstraintLayout>